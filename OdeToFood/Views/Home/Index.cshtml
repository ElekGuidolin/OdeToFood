@model OdeToFood.ViewModels.HomeIndexViewModel

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <h1>@Model.CurrentMessage</h1>
    <table>
        @foreach (var res in Model.Restaurants)
        {
            <tr>
                <td>@res.Id</td>
                <td>@res.Name</td>
            </tr>
        }
    </table>
</body>
</html>